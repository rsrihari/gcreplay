			 },
			 {
				  "text":"Results of inter-hall debate: Gold-RK Silver-Azad Bronze-SN Individual winners: Kaustabh Khare(RK)-Gold Shushman Choudhary(Azad)- Silver Amrit Anand(RP) and V Suprad Reddy(LBS) Congratulations to the winners.",
				  "link":"https://www.facebook.com/156819310131/posts/10151284853110132",
				  "time":"Nov 3, '12"
			 },
		]
	};
	
	for (var i=0; i<JSONObject.events.length; i++) {
	console.log('choot');
		var region = document.getElementById('sexy');
		var textPop = JSONObject.events[i].text;
		var linkPop = JSONObject.events[i].link;
		var timePop = JSONObject.events[i].time;
		
		var newElement = document.createElement('a');
		var divId = 'data-'+i;
		newElement.setAttribute('id', divId);
		newElement.setAttribute('data-original-title', timePop);
		newElement.setAttribute('data-content', textPop);		
		//newElement.setAttribute('href', linkPop);				
		newElement.innerHTML = "Event";
		
		region.appendChild(newElement);
		
		$('#'+divId).popover();
	}
				
				
				
